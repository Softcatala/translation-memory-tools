msgid "copyright"
msgstr "copyright"

# A webpage that serves as the starting point of a website.
msgctxt "1824797"
msgid "home page"
msgstr "pàgina d'inici"

# A webpage that serves as the starting point of a website.
msgctxt "1098326"
msgid "home page"
